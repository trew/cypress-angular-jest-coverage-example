{
  "name": "cypress-angular-jest-coverage-example",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prenyc:copy-reports": "mkdir coverage/reports || true",
    "nyc:copy-reports": "cp coverage/cypress/coverage-final.json coverage/reports/cypress.json && cp coverage/jest/coverage-final.json coverage/reports/jest.json",
    "prenyc:merge-reports": "rm -rf .nyc_output && mkdir .nyc_output || mkdir .nyc_output || true",
    "nyc:merge-reports": "nyc merge coverage/reports && mv coverage.json .nyc_output/out.json",
    "coverage-report": "yarn nyc:copy-reports && yarn nyc:merge-reports && nyc report --reporter lcov --report-dir coverage/combined"
  },
  "private": true,
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "report-dir": "coverage/cypress"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.2.0",
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular/cli": "~8.3.19",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@briebug/cypress-schematic": "^2.0.0",
    "@cypress/code-coverage": "^1.10.1",
    "@cypress/webpack-preprocessor": "4.1.1",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.23",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "core-js": "~3.4.1",
    "cypress": "3.6.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^2.0.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "ngx-build-plus": "^9.0.2",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.16",
    "ts-loader": "6.2.1",
    "ts-node": "^8.5.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack": "^4.41.2"
  }
}
